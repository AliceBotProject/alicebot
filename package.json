{
  "name": "alicebot",
  "type": "module",
  "version": "0.11.0",
  "private": true,
  "description": "A simple asynchronous python chat bot framework.",
  "author": "st1020 <stone_1020@qq.com>",
  "license": "MIT",
  "homepage": "https://docs.alicebot.dev/",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AliceBotProject/alicebot.git"
  },
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "changelog": "conventional-changelog -p angular -i docs/changelog.md -s -r 0",
    "markdownlint": "markdownlint-cli2 '{*.md,docs/guide/**/*.md,packages/**/*.md}'",
    "pyright": "pyright",
    "prettier": "prettier -c .",
    "prettier:fix": "prettier -w .",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@iconify-json/mdi": "^1.2.3",
    "unocss": "^66.0.0",
    "vitepress": "^1.6.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.14.1",
    "@unocss/eslint-config": "^66.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "markdownlint-cli2": "^0.17.2",
    "prettier": "^3.5.3",
    "pyright": "^1.1.399",
    "typescript-eslint": "^8.30.1",
    "vue-eslint-parser": "^10.1.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  }
}
